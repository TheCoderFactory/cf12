<%= simple_form_for(@feedback_form) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :course_id %>
    <%= f.input :location_id %>
    <%= f.input :satisfaction %>
    <%= f.input :recommend %>
    <%= f.input :another %>
    <%= f.input :achieve %>
    <%= f.input :value %>
    <%= f.input :best_part %>
    <%= f.input :comments %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
