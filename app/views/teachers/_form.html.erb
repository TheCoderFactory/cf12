<%= simple_form_for(@teacher) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :image %>
    <%= f.input :title %>
    <%= f.input :link %>
    <%= f.input :blurb %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
